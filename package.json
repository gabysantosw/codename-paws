{
  "name": "codename-paws",
  "version": "0.0.0",
  "private": true,
  "description": "Website to make it easier to find local animals to adopt and support organizations / individuals that rescue and take care of them.",
  "scripts": {
    "start": "node ./bin/www",
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "./**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabysantosw/codename-paws.git"
  },
  "keywords": [],
  "author": "Gabriela S. Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabysantosw/codename-paws/issues"
  },
  "homepage": "https://github.com/gabysantosw/codename-paws#readme",
  "dependencies": {
    "cfonts": "^2.8.6",
    "colors": "^1.4.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "pug": "3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  }
}
